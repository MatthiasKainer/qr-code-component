{
  "name": "qr-code-webcomponent",
  "version": "0.0.1",
  "description": "A simple qr code web component",
  "main": "dist/qr-code.min.js",
  "module": "dist/qr-code.min.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch & rollup -c --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "npm run build:watch & es-dev-server --node-resolve --watch",
    "test": "npm run build && karma start karma.conf.cjs",
    "test:watch": "tsc --watch & karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "checksize": "rollup -c ; cat qr-code.min.js | gzip -9 | wc -c ;"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "Matthias Kainer",
  "license": "BSD 3-Clause",
  "dependencies": {
    "lit-element": "^2.3.1",
    "qrjs2": "^0.1.7"
  },
  "devDependencies": {
    "@open-wc/karma-esm": "^2.13.28",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.17",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "karma": "^5.0.4",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^2.0.1",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.8.3",
    "web-component-analyzer": "^1.0.3"
  }
}
