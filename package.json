{
  "name": "qr-code-webcomponent",
  "version": "0.0.1",
  "description": "A simple qr code web component",
  "main": "dist/qr-code.min.js",
  "module": "dist/qr-code.min.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "npm run build && es-dev-server --node-resolve --watch",
    "docs": "es-dev-server --node-resolve --watch",
    "test": "npm run build && karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "Matthias Kainer",
  "license": "BSD 3-Clause",
  "dependencies": {
    "lit-element": "^2.5.1",
    "qrjs2": "^0.1.9"
  },
  "devDependencies": {
    "@open-wc/karma-esm": "^2.16.18",
    "@open-wc/testing": "^2.5.33",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.3.4",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.60.2",
    "eslint": "^6.8.0",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "lit-analyzer": "^1.2.1",
    "mocha": "^7.2.0",
    "prettier": "^2.3.0",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9",
    "web-component-analyzer": "^1.1.6"
  }
}
